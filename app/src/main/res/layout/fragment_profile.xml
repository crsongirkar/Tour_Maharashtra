<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>

    <LinearLayout
        android:id="@+id/linearColumnprofile"
        style="@style/groupStylewhite_A700"
        android:layout_width="407dp"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_36pxv"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginBottom="@dimen/_13pxv"
        android:gravity="center_horizontal"
        android:orientation="vertical">


        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="421dp"
            android:layout_height="677dp"
            android:layout_weight="1"
            android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="start">

            <LinearLayout
                android:id="@+id/linearProfile"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:orientation="vertical">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnProfile"
                    style="@style/btnGradientRounded_1"
                    android:layout_width="@dimen/_226pxh"
                    android:layout_height="@dimen/_68pxv"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_profile"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_profile" />

                <FrameLayout
                    android:id="@+id/frameMemoji"
                    style="@style/groupStyleblue_1"
                    android:layout_width="@dimen/_206pxh"
                    android:layout_height="@dimen/_195pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_20pxh">

                    <ImageView
                        android:id="@+id/imageMemoji"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_183pxv"
                        android:layout_gravity="bottom|start"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_memoji"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_memoji" />
                </FrameLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/username"
                    style="@style/txtDMSansbold22_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_chinmay"
                    tools:text="@string/lbl_chinmay" />

                <TextView
                    android:id="@+id/txtEmail"
                    style="@style/txtDMSansbold20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:singleLine="true"
                    android:text="@string/msg_crsongirkar2_gm"
                    tools:text="@string/msg_crsongirkar2_gm" />

                <View
                    android:id="@+id/lineLineFour"
                    android:layout_width="402dp"
                    android:layout_height="2dp"
                    android:layout_gravity="end"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:background="@color/black_900_7e" />

                <LinearLayout
                    android:id="@+id/linearColumncloudnotif"
                    android:layout_width="match_parent"
                    android:layout_height="515dp"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:gravity="start"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearRowarrowright"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:gravity="end"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowcloudnotif"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_weight="0.95"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btn_weather"
                                style="@style/txtMontserratromansemibold20"
                                android:layout_width="122dp"
                                android:layout_height="49dp"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.87"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:singleLine="true"
                                android:text="@string/lbl_weather"
                                tools:text="@string/lbl_weather" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageArrowright"
                            android:layout_width="@dimen/_7pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_250pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowright" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowarrowrightOne"
                        android:layout_width="407dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_41pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:gravity="end"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowsignal"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.96"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/txtHotelBooking"
                                android:layout_width="180dp"
                                android:layout_height="43dp"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.87"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:singleLine="true"
                                android:text="@string/lbl_hotel_booking"
                                tools:text="@string/lbl_hotel_booking" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageArrowrightOne"
                            android:layout_width="@dimen/_7pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_189pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowright" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowarrowrightTwo"
                        android:layout_width="match_parent"
                        android:layout_height="73dp"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_38pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:gravity="end"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowmenu"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.93"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/btn_faq"
                                style="@style/txtMontserratromansemibold20"
                                android:layout_width="128dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.87"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:singleLine="true"
                                android:text="@string/lbl_faq"
                                tools:text="@string/lbl_faq" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageArrowrightTwo"
                            android:layout_width="@dimen/_7pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_291pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowright" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowarrowrightThree"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:gravity="end"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowupload"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.95"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/txtLanguage"
                                style="@style/txtMontserratromansemibold20"
                                android:layout_width="172dp"
                                android:layout_height="49dp"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.87"
                                android:background="@drawable/bottom_bg"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:singleLine="true"
                                android:text="@string/lbl_language"
                                tools:text="@string/lbl_language" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageArrowrightThree"
                            android:layout_width="@dimen/_7pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_233pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowright" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowarrowrightFour"
                        android:layout_width="match_parent"
                        android:layout_height="64dp"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:gravity="end"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowarrowleft"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_weight="0.95"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/Log_Out"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bottom_bg"
                                android:text="Log_Out"
                                android:textSize="18dp">

                            </Button>


                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageArrowrightFour"
                            android:layout_width="@dimen/_7pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_253pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowright" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:id="@+id/lineLineThree"
                    android:layout_width="@dimen/_418pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_86pxv"
                    android:background="@color/gray_200" />

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="98dp"
                    android:contentDescription="@string/Profile">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" />
                </ScrollView>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>

